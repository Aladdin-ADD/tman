  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thunks/tman/issues"
  },
  "homepage": "https://github.com/thunks/tman",
  "dependencies": {
    "commander": "^2.9.0",
    "glob": "^7.0.3",
    "supports-color": "^3.1.2",
    "thunks": "^4.1.7"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
